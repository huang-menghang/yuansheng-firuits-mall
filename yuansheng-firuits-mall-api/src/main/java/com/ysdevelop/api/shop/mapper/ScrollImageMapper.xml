<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ysdevelop.api.shop.mapper.ScrollImageDao">
   
   <select id="list" resultType="com.ysdevelop.api.entity.ScrollImage">
    select 
    `id`,`name`,`imageHref`,`imageUrl`,`description`
    from `t_ys_firuits_scroll_image`
    order by `sort` desc
    limit 0,3		
   </select>
   
   
   
</mapper>
